{
  "name": "lantern",
  "version": "0.1.0",
  "description": "Diagnose memory issues in Chrome",
  "main": "dist/lantern.js",
  "scripts": {
    "build": "webpack --config webpack.config.js && webpack --config webpack.config.min.js",
    "build-demo": "cd demo/requirejs && bower install && cd ../vanillajs && bower install",
    "clean": "rm -rf dist",
